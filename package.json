{
  "name": "studyhub",
  "version": "1.0.0",
  "description": "Full-stack web platform for students to upload, download, and purchase study notes",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd backend && npm run dev",
    "client:dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "setup:backend": "cd backend && npm install",
    "setup:frontend": "cd frontend && npm install"
  },
  "keywords": ["studyhub", "notes", "education", "fullstack"],
  "author": "StudyHub Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}